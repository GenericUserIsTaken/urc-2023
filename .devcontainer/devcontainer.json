{
    "context": "../",
    "dockerFile": "Dockerfile",
    "containerUser": "root",
    "remoteUser": "root",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt", "seccomp=unconfined"
    ],
    "extensions": [
        "ms-azuretools.vscode-docker",
		"ms-python.python",
		"ms-vscode.cmake-tools",
		"ms-vscode.cpptools",
		"twxs.cmake"
	]
}